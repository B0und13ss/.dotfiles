window {
  location: northeast;
  width: 140px;
  x-offset: -4px;
  y-offset: 4px;
  border-radius: 4px;
}

inputbar {
  enabled: false;
}

listview {
  columns: 1;
  lines: 2;
  padding: 0px;
}

element {
  children: [ element-text ];
  padding: 0px;
  width: 10px;
  border-radius: 4px;
}

element-text {
  horizontal-align: 0.5;
}

element-icon {
  enabled: false;
}
